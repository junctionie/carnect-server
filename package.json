{
  "name": "express-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:back": "npx sequelize-cli db:migrate:undo:all",
    "dev": " nodemon ./bin/www --delay 2"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-async-handler": "^1.1.4",
    "express-validator": "^6.11.1",
    "http-status-codes": "^2.1.4",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-kakao": "^1.0.1",
    "pm2": "^4.5.6",
    "sequelize": "^6.6.2",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "2.3.0",
    "sequelize-cli": "^6.2.0"
  }
